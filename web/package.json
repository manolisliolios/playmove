{
  "name": "@mliolios/playmove-react",
  "version": "0.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:component": "vite build --config vite.component.config.mts",
    "npmDeployment": "pnpm build:component && npm publish"
  },
  "main": "./dist/playmove.cjs.js",
  "module": "./dist/playmove.es.js",
  "types": "./dist/playmove.d.ts",
  "exports": {
    ".": {
      "types": "./dist/playmove.d.ts",
      "import": "./dist/playmove.es.js",
      "require": "./dist/playmove.cjs.js"
    },
    "./style.css": "./dist/playmove-react.css"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "src"
  ],
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/themes": "^3.2.1",
    "@shikijs/monaco": "^3.7.0",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-query": "^5.71.3",
    "ansi-to-react": "^6.1.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.518.0",
    "monaco-editor": "^0.52.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^3.0.3",
    "shiki": "^3.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@shikijs/monaco": "^3.7.0",
    "monaco-editor": "^0.52.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shiki": "^3.7.0"
  }
}
